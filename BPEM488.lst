                                      ;***************************************************************************************** (IGN_CALCS_512)
00522E 7F522E FC 10 37                        ldd   Spantk     ; "Spantk" -> Accu D                                    (IGN_CALCS_512)
005231 7F5231 B3 10 3D                        subd  Advancetk  ; Subtract (A:B)-(M:M+1)=>A:B "Spantk" - "Advancetk" = "Delaytk" (IGN_CALCS_512)
005234 7F5234 7C 10 3F                        std   Delaytk    ; Copy result to "Delaytk"                              (IGN_CALCS_512)
005237 7F5237 7C 10 41                        std   IgnOCadd1  ; Copy result to "IgnOCadd1"                            (IGN_CALCS_512)
00523A 7F523A 20 65                           bra  IgnCalcsDone  ; Branch to IgnCalcsDone:
