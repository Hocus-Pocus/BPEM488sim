005029 7F5029 CC 51 0B                    ldd  #$510B         ; Load accu D with Lo word of KPHk                       (CALC_KPH)
00502C 7F502C CD 00 04                    ldy  #$0004         ; Load accu Y with Hi word of KPHk                       (CALC_KPH)
00502F 7F502F FE 10 AA                    ldx  VSSprd         ; Load "X" register with value in "VSSprd"               (CALC_KPH)
005032 7F5032 11                          ediv                ; Extended divide (Y:D)/(X)=>Y;Rem=>D (Divide "KPHk" by "VSSprd") (CALC_KPH)
005033 7F5033 7D 10 AE                    sty  KPH            ; Copy result to "KPH"                                   (CALC_KPH)
005036 7F5036 1D 10 24 02                 bclr ICflgs,KPHcalc ; Clear "KPHcalc" bit of "ICflgs"                        (CALC_KPH)
