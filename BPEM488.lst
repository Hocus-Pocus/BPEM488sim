                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
0059B2 7F59B2                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
0059B2 7F59B2                         Inc125uS:
0059B2 7F59B2 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
0059B5 7F59B5 B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
0059B8 7F59B8 81 08                       cmpa #$08          ; Compare it with decimal 8
0059BA 7F59BA 26 54                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0059BC 7F59BC                         DomS:
0059BC 7F59BC 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
0059C0 7F59C0 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
0059C3 7F59C3 72 10 03                    inc  mS            ; Increment Millisecond counter
0059C6 7F59C6 B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
0059C9 7F59C9 81 64                       cmpa #$64          ; Compare it with decimal 100
0059CB 7F59CB 27 0A                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
0059CD 7F59CD 81 C8                       cmpa #$C8          ; Compare it with decimal 200
0059CF 7F59CF 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=200)
0059D1 7F59D1 81 FA                       cmpa #$FA          ; Compare it with decimal 250
0059D3 7F59D3 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
0059D5 7F59D5 26 39                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
0059D7 7F59D7                         Do100mS:
0059D7 7F59D7 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
0059DB 7F59DB 20 33                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0059DD 7F59DD                         Do250mS:
0059DD 7F59DD 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
                                      ;   if it's time to do the "500mS" section.
                                      ;*****************************************************************************************
0059E1 7F59E1 79 10 03                    clr  mS            ; Clear Millisecond counter
0059E4 7F59E4 72 10 04                    inc  mSx250        ; Increment 250 Millisecond counter
0059E7 7F59E7 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
0059EA 7F59EA 81 02                       cmpa #$02          ; Compare with decimal 2
0059EC 7F59EC 27 06                       beq  Do500mS       ; If the Z bit of CCR is set, branch to Do500mS:
0059EE 7F59EE 81 04                       cmpa #$04          ; Compare with decimal 4
0059F0 7F59F0 27 02                       beq  Do500mS       ; If the Z bit of CCR is set,branch to Do500mS:
0059F2 7F59F2 20 1C                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 500 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0059F4 7F59F4                         Do500mS:
0059F4 7F59F4 1C 10 05 08                 bset clock,ms500   ; Set "ms500" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Check to see if it's time to do the "Seconds" section
                                      ;*****************************************************************************************
0059F8 7F59F8 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
0059FB 7F59FB 81 04                       cmpa #$04          ; Compare with decimal 4
0059FD 7F59FD 27 02                       beq  DoSec         ; If the Z bit of CCR is set, branch to DoSec:
0059FF 7F59FF 20 0F                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ---------------------------------- Seconds section -------------------------------------
                                      ;*****************************************************************************************
005A01 7F5A01                         DoSec:
005A01 7F5A01 1C 10 05 10                 bset clock,ms1000     ; Set "ms1000" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 250 millisecond counter. Increment "secL". Increment "secH" on roll over
                                      ;*****************************************************************************************
005A05 7F5A05                         IncSec:
005A05 7F5A05 79 10 04                    clr  mSx250        ; Clear 250 mSec counter
005A08 7F5A08 72 10 65                    inc  secL          ; Increment "Seconds" Lo byte
005A0B 7F5A0B 26 03                       bne  RTI_ISR_DONE  ; If the Z bit of CCR is clear, branch to RTI_ISR_DONE:
005A0D 7F5A0D 72 10 64                    inc  secH          ; Increment "Seconds" Hi byte
005A10 7F5A10                         RTI_ISR_DONE:
005A10 7F5A10 4C 37 80                    bset CRGFLG,RTIF   ; Set "RTIF" bit of "CRGFLG" to clear flag
005A13 7F5A13 0B                          rti                ; Return from interrupt
