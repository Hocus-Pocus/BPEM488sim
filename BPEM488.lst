005504 7F5504 CD 27 10                        ldy  #$2710        ; Load index register Y with decimal 10000 (for integer math) (RUN_PW_CALCS)
005507 7F5507 13                              emul               ;(D)x(Y)=Y:D "PWtk" * 10,000                          (RUN_PW_CALCS)
005508 7F5508 FE 10 A8                        ldx  CASprd256     ; "CASprd256"-> Accu X                                (RUN_PW_CALCS)
00550B 7F550B 11                          ediv               ;(Y:D)/(X)=Y;Rem->D ("PWtk"*10,000)/"CASprd256"           (RUN_PW_CALCS)
00550C 7F550C B7 54                           tfr  X,D           ; Result-> Accu D                                     (RUN_PW_CALCS)
00550E 7F550E CE 00 64                        ldx  #$0064        ; Decimal 100 -> Accu X                               (RUN_PW_CALCS)
005511 7F5511 18 10                       idiv               ; (D)/(X)->(X)rem(D) (("PWtk"*10,000)/"CASprd256")/100    (RUN_PW_CALCS)
                                                                 ; ="DutyCyclex10"                                     (RUN_PW_CALCS)
005513 7F5513 7E 10 F6                    stx  DutyCyclex10  ; Copy result to "DutyCyclex10" (Injector duty cycle x 10) (RUN_PW_CALCS)
