                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
00619D 7F619D                         ReEntCrvPos:
00619D 7F619D 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
0061A0 7F61A0 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
0061A6 7F61A6 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
0061AA 7F61AA FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
0061AD 7F61AD FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
0061B0 7F61B0 19 EE                       leay D,Y          ; Curve row pointer -> Y
0061B2 7F61B2 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
