005A5E 7F5A5E 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
005A61 7F5A61 0B                          rti                  ; Return from interrupt
005A62 7F5A62                         RcvOSmsb:                ; "rxmode" = 3
005A62 7F5A62 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Offset MSB)
005A64 7F5A64 7A 10 0A                    staa  rxoffsetMSB    ; Copy to "rxoffsetMSB"
005A67 7F5A67 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
005A6A 7F5A6A 0B                          rti                  ; Return from interrupt
005A6B 7F5A6B                         RcvOSlsb:                ; "rxmode" = 4
005A6B 7F5A6B 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Offset LSB)
005A6D 7F5A6D 7A 10 0B                    staa  rxoffsetLSB    ; Copy to "rxoffsetLSB"
005A70 7F5A70 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
005A73 7F5A73 0B                          rti                  ; Return from interrupt
005A74 7F5A74                         RcvCntmsb:               ; "rxmode" = 5
005A74 7F5A74 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Byte count MSB)
005A76 7F5A76 7A 10 06                    staa  txgoalMSB      ; Copy to "txgoalMSB"
005A79 7F5A79 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
005A7C 7F5A7C 0B                          rti                  ; Return from interrupt
005A7D 7F5A7D                         RcvCntlsb:               ; "rxmode" = 6
005A7D 7F5A7D 96 CF                       ldaa  SCI0DRL        ; Load Accu A with value in "SCI0DRL" (Byte count LSB)
005A7F 7F5A7F 7A 10 07                    staa  txgoalLSB      ; Copy to "txgoalLSB"
005A82 7F5A82 B6 10 0F                    ldaa  txcmnd         ; Load Accu A with value in "txcmnd"
005A85 7F5A85 81 01                       cmpa  #$01           ; Compare with decimal 1 ("C")
005A87 7F5A87 18 27 01 36                 beq   ModeC2         ; If the Z bit of CCR is set, branch to ModeC2:
005A8B 7F5A8B 72 10 0C                    inc   rxmode         ; Increment "rxmode"(continue to next mode)
005A8E 7F5A8E 0B                          rti                  ; Return from interrupt (ready to receive next byte)
005A8F 7F5A8F                         RcvData:                 ; "rxmode" = 7
                                      ;*****************************************************************************************
                                      ; - If we are here we must be in "W" mode and receiving either one or
                                      ;   two bytes, depending on the byte count.
                                      ;*****************************************************************************************
005A8F 7F5A8F FC 10 06                    ldd  txgoalMSB      ; Load double accumulator with value in
                                                              ; "txgoalMSB:txgoalLSB"
005A92 7F5A92 8C 00 02                    cpd  #$0002         ; Compare with decimal 2
005A95 7F5A95 27 05                       beq  RcvDataMSB     ; If equal branch to RcvDataMSB:
005A97 7F5A97 8C 00 01                    cpd  #$0001         ; Compare with decimal 1
005A9A 7F5A9A 27 09                       beq  RcvDataLSB     ; If equal branch to RcvDataLSB:
005A9C 7F5A9C                         RcvDataMSB:
005A9C 7F5A9C 96 CF                       ldaa  SCI0DRL       ; Load Accu A with value in "SCI0DRL"(data byte)
005A9E 7F5A9E 7A 10 10                    staa  dataMSB       ; Copy to "dataMSB"
005AA1 7F5AA1 72 10 0C                    inc   rxmode        ; Increment "rxmode"(continue to next mode)
005AA4 7F5AA4 0B                          rti                 ; Return from subroutine
005AA5 7F5AA5                         RcvDataLSB:             ; "rxmode" = 8
005AA5 7F5AA5 D6 CF                       ldab  SCI0DRL       ; Load Accu B with value in "SCI0DRL"(data byte)
005AA7 7F5AA7 7B 10 11                    stab  dataLSB       ; Copy to "dataLSB"
005AAA 7F5AAA B6 10 0E                    ldaa  pageID        ; Load accu A with value in "pageID"
005AAD 7F5AAD 81 01                       cmpa  #$01          ; Compare with decimal 1 (send page 1)
005AAF 7F5AAF 27 0A                       beq   StorePg1      ; If the Z bit of CCR is set, branch to StorePg1:
005AB1 7F5AB1 81 02                       cmpa  #$02          ; Compare with decimal 2 (send page 2)
005AB3 7F5AB3 27 4B                       beq   StorePg2      ; If the Z bit of CCR is set, branch to StorePg2:
005AB5 7F5AB5 81 03                       cmpa  #$03          ; Compare with decimal 3 (send page 3)
005AB7 7F5AB7 18 27 00 86                 beq   StorePg3      ; If the Z bit of CCR is set, branch to StorePg3:
005ABB 7F5ABB                         StorePg1:
