00626B 7F626B FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
00626E 7F626E 19 EE                       leay D,Y          ; Curve row pointer -> Y
006270 7F6270 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
