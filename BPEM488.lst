                                      ;*****************************************************************************************
                                      ; - In the INIT_TIM macro, Port T PT0, PT2 and all Port P pins are set as outputs with
                                      ;   initial setting low. To control both the ignition and injector drivers two interrupts
                                      ;   are required for each ignition or injection event. At the appropriate crank angle and
                                      ;   cam phase an interrupt is triggered. In this ISR routine the channel output compare
                                      ;   register is loaded with the delay value from trigger time to the time desired to
                                      ;   energise the coil or injector and the channel interrupt is enabled. When the output
                                      ;   compare matches, the pin is commanded high and the timer channel interrupt is triggered.
                                      ;   The output compare register is then loaded with the value to keep the coil or injector
                                      ;   energised, and the channel interrupt is disabled. When the output compare matches, the
                                      ;   pin is commanded low to fire the coil or de-energise the injector.
                                      ;*****************************************************************************************
005C39 7F5C39                         TIM_TC0_ISR:
                                      ;*****************************************************************************************
                                      ; - TIM ch1 Interrupt Service Routine (for (D21)(1to28)(Ign3)(9&8) control)
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Set the output compare value for desired on time and disable the interrupt
                                      ;*****************************************************************************************
005C39 7F5C39 1C 03 D9 02                 bset TIM_TCTL2,Bit1    ; Clear Ch0 output line to zero on compare
005C3D 7F5C3D 1D 03 D9 01                 bclr TIM_TCTL2,Bit0    ; Clear Ch0 output line to zero on compare
005C41 7F5C41 FC 03 D4                    ldd  TIM_TCNTH         ; Contents of Timer Count Register-> Accu D
005C44 7F5C44 F3 10 43                    addd IgnOCadd2         ; Add "IgnOCadd2" (dwell time)
005C47 7F5C47 7C 03 E0                    std  TIM_TC0H          ; Copy result to Timer IC/OC register 1 (Start OC operation)
                                                                 ; (coil on for dwell time)(LED on)
005C4A 7F5C4A 0B                          rti                    ; Return from Interrupt
005C4B 7F5C4B                         TIM_TC1_ISR:
                                      ;*****************************************************************************************
                                      ; - TIM ch1 Interrupt Service Routine (for (D5)(1to28)(Ign4)(4&7) control)
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Set the output compare value for desired on time and disable the interrupt
                                      ;*****************************************************************************************
005C4B 7F5C4B 1C 03 D9 08                 bset TIM_TCTL2,Bit3    ; Clear Ch1 output line to zero on compare
005C4F 7F5C4F 1D 03 D9 04                 bclr TIM_TCTL2,Bit2    ; Clear Ch1 output line to zero on compare
005C53 7F5C53 FC 03 D4                    ldd  TIM_TCNTH         ; Contents of Timer Count Register-> Accu D
005C56 7F5C56 F3 10 43                    addd IgnOCadd2         ; Add "IgnOCadd2" (dwell time))
005C59 7F5C59 7C 03 E2                    std  TIM_TC1H          ; Copy result to Timer IC/OC register 1 (Start OC operation)
                                                                 ; (coil on for dwell time)(LED on)
005C5C 7F5C5C 0B                          rti                    ; Return from Interrupt
005C5D 7F5C5D                         TIM_TC2_ISR:
                                      ;*****************************************************************************************
                                      ; - TIM ch2 Interrupt Service Routine (for (D24)(1to28)(Ign5)(3&2) control)
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Set the output compare value for desired on time and disable the interrupt
                                      ;*****************************************************************************************
005C5D 7F5C5D 1C 03 D9 20                 bset TIM_TCTL2,Bit5    ; Clear Ch2 output line to zero on compare
005C61 7F5C61 1D 03 D9 10                 bclr TIM_TCTL2,Bit4    ; Clear Ch2 output line to zero on compare
005C65 7F5C65 FC 03 D4                    ldd  TIM_TCNTH         ; Contents of Timer Count Register-> Accu D
005C68 7F5C68 F3 10 43                    addd IgnOCadd2         ; Add "IgnOCadd2" (dwell time)
005C6B 7F5C6B 7C 03 E4                    std  TIM_TC2H          ; Copy result to Timer IC/OC register 2 (Start OC operation)
                                                                 ; (coil on for dwell time)(LED on)
005C6E 7F5C6E 0B                          rti                    ; Return from Interrupt
005C6F 7F5C6F                         TIM_TC3_ISR:
                                      ;*****************************************************************************************
                                      ; - TIM ch3 Interrupt Service Routine (for (D1)(1to28)(Inj1)(1&10) control)
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Set the output compare value for desired on time and disable the interrupt
                                      ;*****************************************************************************************
005C6F 7F5C6F 1C 03 D9 80                 bset TIM_TCTL2,Bit7    ; Clear Ch3 output line to zero on compare
005C73 7F5C73 1D 03 D9 40                 bclr TIM_TCTL2,Bit6    ; Clear Ch3 output line to zero on compare
005C77 7F5C77 FC 03 D4                    ldd  TIM_TCNTH         ; Contents of Timer Count Register-> Accu D
005C7A 7F5C7A F3 10 61                    addd InjOCadd2         ; Add "InjOCadd2" (injector pulse width)
005C7D 7F5C7D 7C 03 E6                    std  TIM_TC3H          ; Copy result to Timer IC/OC register 3 (Start OC operation)
                                                                 ; (Should result in LED on for ~3 to ~25 mS)
                                      ;***********************************************************************************************
                                      ; - Update Fuel Delivery Pulse Width Total so the results can be used by Tuner Studio and
                                      ;   Shadow Dash to calculate current fuel burn.
                                      ;***********************************************************************************************
005C80 7F5C80 FC 10 D6                    ldd  FD     ; "FD"->Accu D (fuel delivery pulsewidth)(mS)
005C83 7F5C83 F3 10 63                        addd FDt    ; (A:B)+(M:M+1)->A:B (fuel delivery pulsewidth + fuel delivery pulsewidth total)
005C86 7F5C86 7C 10 63                        std  FDt    ; Copy result to "FDt" (mS)
                                      ;***********************************************************************************************
                                      ; - Update the Fuel Delivery counter so that on roll over (65535mS)a pulsed signal can be sent to the
                                      ;   to the totalizer(open collector output)
                                      ;***********************************************************************************************
005C89 7F5C89 FC 10 D6                    ldd  FD             ; "FD"->Accu D (fuel delivery pulsewidth)(mS)
005C8C 7F5C8C F3 10 65                        addd FDcnt          ; (A:B)+(M:M+1)->A:B (fuel delivery pulsewidth + fuel delivery counter)
005C8F 7F5C8F 25 05                       bcs  Totalizer1     ; If the cary bit of CCR is set, branch to Totalizer1: ("FDcnt"
                                                                  ;  rollover, pulse the totalizer)
005C91 7F5C91 7C 10 65                        std  FDcnt          ; Copy the result to "FDcnt" (update "FDcnt")
005C94 7F5C94 20 0B                       bra  TotalizerDone1 ; Branch to TotalizerDone1:
005C96 7F5C96                         Totalizer1:
005C96 7F5C96 7C 10 65                        std  FDcnt          ; Copy the result to "FDcnt" (update "FDcnt")
005C99 7F5C99 4C 01 40                    bset PORTB,AIOT     ; Set "AIOT" pin on Port B (PB6)(start totalizer pulse)
005C9C 7F5C9C 86 03                           ldaa #$03           ; Decimal 3->Accu A (3 mS)
