005FFF 7F5FFF 19 EA 02 AC                             LEAY 3DLUT_COL_BIN_OFFSET,Y   ; Column bin pointer -> Y
                                                                            ;($2AC=684)
006003 7F6003 C6 22                           LDAB #(2*(3DLUT_ROW_COUNT-1)) ; Lower column bin offset -> B
                                                                            ;($22=34)
006005 7F6005 18 0F                           TBA                                     ; Lower  offset -> A (start at $22=34)
006007 7F6007 AE EC                           CPX      A,Y            ; Compare column value against current bin
                                                                  ; value
006009 7F6009 2C 10                           BGE  3D_LOOKUP_2A   ; First iteration, if equal to or greater
                                                                  ; than current bin value, rail high, upper
                                                                  ; and lower bin offsets the same
00600B 7F600B                         3D_LOOKUP_1:
00600B 7F600B 18 0F                           TBA                                     ; Lower  offset -> A
00600D 7F600D AE EC                           CPX      A,Y            ; Compare column value against current bin
                                                                  ; value
00600F 7F600F 2C 08                                   BGE      3D_LOOKUP_2    ; Branch if column value is greater than
                                                                  ; or equal to current bin value
                                                                  ;(match found)
006011 7F6011 53                                      DECB                ; Decrement bin offset low byte
006012 7F6012 04 31 F6                                DBNE B,3D_LOOKUP_1  ; Decrement bin offset Hi byte and loop
                                                                  ; back if not zero
006015 7F6015 18 0F                                   TBA                                     ; Column value too low, no match found,
                                                                  ; rail low, make lower and upper bin
                                                                  ; offsets the same)
006017 7F6017 20 02                           BRA   3D_LOOKUP_2A
