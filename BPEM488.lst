                                      ;*****************************************************************************************
00627E 7F627E                         ReEntCrvPos1:
00627E 7F627E 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
006281 7F6281 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
006287 7F6287 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
00628B 7F628B FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
00628E 7F628E FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
006291 7F6291 19 EE                       leay D,Y          ; Curve row pointer -> Y
006293 7F6293 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
                                                            ; (holds the contents of the incremented row bin)
