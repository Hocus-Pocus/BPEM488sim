005DC9 7F5DC9                                                 ORG     STATE_CODE_START, STATE_CODE_START_LIN
005DC9 7F5DC9 -> $7F5DC9              STATE_CODE_START_LIN    EQU     @ ; @ Represents the current value of the linear
                                                                    ; program counter
                                      ;*****************************************************************************************
                                      ; - The camshaft position sensor and the crankshaft position sensor are both hall
                                      ;   effect gear tooth sensors. They read notched wheels on their repsective shafts.
                                      ;   When the sensor senses a notch its output pin goes to ground. The BPEM simulator
                                      ;   input from the sensor is the LED circuit of an opto isolator. When the LED in the
                                      ;   opto is powered it biases the output transistor on so the timer channel pin sees a
                                      ;   rising edge, which triggers an interrupt event. The state machine uses these events
                                      ;   to de-code the signals to determine crankshaft position and camshaft phase. Any
                                      ;   event that does not fall into the mechanical order of events triggers an error.
                                      ;   An error will disable ignition and fuel injection until a positive lock on crankshaft
                                      ;   position and camshaft phase is re-established.
                                      ;*****************************************************************************************
005DC9 7F5DC9                         ECT_TC5_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT_TC5_ISR Interrupt Service Routine (Camshaft sensor notch)
                                      ;   Event = 0
                                      ;*****************************************************************************************
005DC9 7F5DC9 DE 5A                       ldx    ECT_TC5H         ; Read ECT_TC5H to clear the flag
005DCB 7F5DCB CE 6E BE                    ldx    #StateLookup     ; Load index register X with the address of "TableLookup"
005DCE 7F5DCE F6 10 14                    ldab   State            ; Load Accu B with the contents of "State"
005DD1 7F5DD1 58                          aslb                    ; Shift Accu B 1 place to the left
005DD2 7F5DD2 CA 00                       orab   #$00             ; Bit wise inclusive OR Accu B with 0
005DD4 7F5DD4 A6 E5                       ldaa   B,X              ; Load Accu A with the contents of "TableLookup", offset in
                                                                  ; Accu B (9 bit constant offset indexed addressing)
005DD6 7F5DD6 7A 10 14                    staa   State            ; Copy to "State"
005DD9 7F5DD9 0B                          rti                     ; Return from interrupt
005DDA 7F5DDA                         ECT_TC7_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT_TC7_ISR Interrupt Service Routine (Crankshaft sensor notch)
                                      ;   Event = 1
                                      ;*****************************************************************************************
005DDA 7F5DDA DE 5E                       ldx    ECT_TC7H              ; Read ECT_TC7H to clear the flag
005DDC 7F5DDC CE 6E BE                    ldx    #StateLookup     ; Load index register X with the address of "TableLookup"
005DDF 7F5DDF F6 10 14                    ldab   State            ; Load Accu B with the contents of "State"
005DE2 7F5DE2 58                          aslb                    ; Shift Accu B 1 place to the left
005DE3 7F5DE3 CA 01                       orab   #$01             ; Bit wise inclusive OR Accu B with 1
005DE5 7F5DE5 A6 E5                       ldaa   B,X              ; Load Accu A with the contents of "TableLookup", offset in
                                                                  ; Accu B (9 bit constant offset indexed addressing)
005DE7 7F5DE7 7A 10 14                    staa   State            ; Copy to "State"
005DEA 7F5DEA 81 46                       cmpa    #$46            ; Compare with decimal 70 (Error)
005DEC 7F5DEC 27 06                       beq     State_Error     ; If "State" = $46, branch to State_Error:
005DEE 7F5DEE 81 67                       cmpa    #$67            ; Compare with decimal 103
005DF0 7F5DF0 2F 21                       ble     NoLock          ; If "State" =< $67, branch to NoLock:
005DF2 7F5DF2 2E 11                       bgt     SynchLock       ; If "State" is > $67, branch to Synchlock:
005DF4 7F5DF4                         State_Error:
                                      ;*****************************************************************************************
                                      ; - If we get here we have experienced an unexpected cam or crank input and have lost lock.
                                      ;   No more spark or injection events until lock has been re-established.
                                      ;*****************************************************************************************
005DF4 7F5DF4 79 10 14                    clr   State                     ; Clear "State"
005DF7 7F5DF7 1C 10 EF 02                 bset  StateStatus,SynchLost     ; Set "SynchLost" bit of "StateStatus" variable (bit1)
005DFB 7F5DFB 1D 10 EF 01                 bclr  StateStatus,Synch         ; Clear "Synch " bit of "StateStatus" variable (bit0)
005DFF 7F5DFF 1D 10 EF 04                 bclr  StateStatus,StateNew      ; Clear "StateNew " bit of "StateStatus" variable (bit2)
005E03 7F5E03 20 1A                       bra   STATE_STATUS_done         ; Branch to STATE_STATUS_done:
005E05 7F5E05                         SynchLock:
                                      ;*****************************************************************************************
                                      ; - If we get here we have either just reached one of the four possible lock points, or
                                      ;    we are already in the synch loop.
                                      ;*****************************************************************************************
005E05 7F5E05 1C 10 EF 01                 bset  StateStatus,Synch        ; Set "Synch " bit of "StateStatus" variable (bit0)
005E09 7F5E09 1C 10 EF 04                 bset  StateStatus,StateNew     ; Set "StateNew" bit of "StateStatus" variable (bit2)
005E0D 7F5E0D 1D 10 EF 02                 bclr  StateStatus,SynchLost    ; Clear "SynchLost" bit of "StateStatus" variable (bit1)
005E11 7F5E11 20 0C                       bra   STATE_STATUS_done        ; Branch to STATE_STATUS_done:
005E13 7F5E13                         NoLock:
                                      ;*****************************************************************************************
                                      ; - If we get here we have the state machine is still looking for a synch lock.
                                      ;*****************************************************************************************
005E13 7F5E13 1D 10 EF 01                 bclr  StateStatus,Synch        ; Clear "Synch" bit of "StateStatus" variable (bit0)
005E17 7F5E17 1D 10 EF 02                 bclr  StateStatus,SynchLost    ; Clear "SynchLost" bit of "StateStatus" variable (bit1)
005E1B 7F5E1B 1D 10 EF 04                 bclr  StateStatus,StateNew     ; Clear "StateNew" bit of "StateStatus" variable (bit 2)
005E1F 7F5E1F                         STATE_STATUS_done:
                                      ;*****************************************************************************************
                                      ; - Get three consecutive rising edge signals for engine RPM and
                                      ;   calculate the period. This period is for one fifth of a revolution (72 degrees).
                                      ;   RPM, Ignition and  Fuel calculations are done in the main loop.
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Reload stall counter with compare value. Stall check is done in the main loop every
                                      ;   mSec. "Stallcnt" is decremented every mSec and reloaded at every crank signal.
                                      ;*****************************************************************************************
005E1F 7F5E1F 18 0B FF 00 17                  movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005E24 7F5E24 CD 08 00                    ldy   #veBins_E       ; Load index register Y with address of first configurable
                                                              ; constant on buffer RAM page 1 (vebins)
005E27 7F5E27 EC EA 03 E6                 ldd   $03E6,Y       ; Load Accu A with value in buffer RAM page 1 offset 998
                                                              ; "Stallcnt" (stall counter)(offset = 998)
005E2B 7F5E2B 7C 10 22                    std  Stallcnt       ; Copy to "Stallcnt" (no crank or stall condition counter)
                                                              ; (1mS increments)
005E2E 7F5E2E 1E 10 24 04 12              brset ICflgs,Ch7_2nd,CAS_2nd ; If "Ch7_2nd" bit of "ICflgs" is set, branch to "CAS_2nd:"
005E33 7F5E33 1E 10 24 10 24              brset ICflgs,Ch7_3d,CAS_3d   ; If "Ch7_3d" bit of "ICflgs" is set, branch to "CAS_3d:"
005E38 7F5E38 DC 5E                       ldd   ECT_TC7H               ; Load accu D with value in "ECT_TC7H"
005E3A 7F5E3A 7C 10 15                    std   CAS1sttk               ; Copy to "CAS1sttk"
005E3D 7F5E3D 1C 10 24 04                 bset  ICflgs,Ch7_2nd         ; Set "Ch7_2nd" bit of "ICflgs"
005E41 7F5E41 18 20 00 C0                 bra   CASDone                ; Branch to CASDone:
005E45 7F5E45                         CAS_2nd:
005E45 7F5E45 DC 5E                       ldd   ECT_TC7H        ; Load accu D with value in "ECT_TC7H"
005E47 7F5E47 7C 10 17                    std   CAS2ndtk        ; Copy to "CAS2ndtk"
005E4A 7F5E4A B3 10 15                    subd  CAS1sttk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "ECT_TC7H"
005E4D 7F5E4D 7C 10 19                    std   CASprd1tk       ; Copy result to "CASprd1tk"
005E50 7F5E50 1D 10 24 04                 bclr  ICflgs,Ch7_2nd  ; Clear "Ch7_2nd" bit of "ICflgs"
005E54 7F5E54 1C 10 24 10                 bset  ICflgs,Ch7_3d   ; Set "Ch7_3d" bit of "ICflgs"
005E58 7F5E58 18 20 00 A9                 bra   CASDone         ; Branch to CASDone:
005E5C 7F5E5C                         CAS_3d:
005E5C 7F5E5C DC 5E                       ldd   ECT_TC7H        ; Load accu D with value in "ECT_TC7H"
005E5E 7F5E5E B3 10 17                    subd  CAS2ndtk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS2ndtk" from value in "ECT_TC7H"
005E61 7F5E61 7C 10 1B                    std   CASprd2tk       ; Copy result to "CASprd2tk"
005E64 7F5E64 F3 10 19                    addd  CASprd1tk       ; (A:B)+(M:M+1)_->A:B "CASprd2tk" + "CASprd1tk" = "CASprdtk"
005E67 7F5E67 1D 10 24 10                 bclr  ICflgs,Ch7_3d   ; Clear "Ch7_3d" bit of "ICflgs"
                                      ;*****************************************************************************************
                                      ; - All calculations that use the Crank Angle Sensor period need to know what the
                                      ;   resolution is. The timers are initalized with a 5.12uS resoluion but switched to
                                      ;   2.56uS resolution when the engine tranistions from crank mode to run mode.
                                      ;*****************************************************************************************
005E6B 7F5E6B 1E 10 EB 04 17              brset engine,run,CAS256 ; If "run" bit of "engine" bit field is set branch to
                                                                    ; CAS256:
005E70 7F5E70 7C 10 A2                    std   CASprd512       ; Copy result to "CASprd512" (CAS period in 5.12uS resolution)
                                      ;******************************************************************************************
                                      ; - Convert Crank Angle Sensor period (5.12uS res)to degrees x 10 of rotation (for 1 tenth
                                      ;   of a degree resolution calculations).("Degx10tk512")
                                      ;******************************************************************************************
005E73 7F5E73 CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005E76 7F5E76 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd512/72)
005E78 7F5E78 B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005E7A 7F5E7A CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E7D 7F5E7D 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk512"
005E7E 7F5E7E 7C 10 1D                        std   Degx10tk512    ; Copy result to "Degx10tk512"
005E81 7F5E81 18 79 10 1F                     clrw  Degx10tk256    ; Clear "Degx10tk256"
005E85 7F5E85 20 15                           bra   CASprdDone     ; Branch to CASprdDone:
005E87 7F5E87                         CAS256:
005E87 7F5E87 7C 10 A4                    std   CASprd256      ; Copy result to "CASprd256" (CAS period in 2.56uS resolution)
                                      ;******************************************************************************************
                                      ; - Convert Crank Angle Sensor period (2.56uS res)to degrees x 10 of rotation (for 1 tenth
                                      ;   of a degree resolution calculations).("Degx10tk256")
                                      ;******************************************************************************************
005E8A 7F5E8A CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005E8D 7F5E8D 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd256/72)
005E8F 7F5E8F B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005E91 7F5E91 CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E94 7F5E94 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk256"
005E95 7F5E95 7C 10 1F                        std   Degx10tk256    ; Copy result to "Degx10tk256"
005E98 7F5E98 18 79 10 1D                 clrw  Degx10tk512    ; Clear "Degx10tk512"
005E9C 7F5E9C                         CASprdDone:
                                      ;*****************************************************************************************
                                      ; - Determine if the engine is cranking or running. The timer is initialized with a
                                      ;   5.12uS time base and the engine status bit field "engine" is cleared on power up.
                                      ;   "Spantk" will roll over at ~85 RPM with a 5.12uS base and at ~169 RPM with a
                                      ;   2.56uS base. The time base is switched from 5.12uS to 2.56uS at ~300 RPM which should
                                      ;   be at a speed when the engine is running. Engine speed can drop to as low as ~169 RPM
                                      ;   before ignition calculations cannot be done. It is not likely that the engine will
                                      ;   continue to run at this speed and will stall. Stall detection is done in the main
                                      ;   loop if the period between crank sensor signals is greater than ~2 seconds.
                                      ;*****************************************************************************************
005E9C 7F5E9C 1E 10 EB 04 60              brset  engine,run,CASprdOK ; If "run" bit of "engine" bit field is set branch to
                                                                    ; CASDone:
                                      ;*****************************************************************************************
                                      ; - Look up the cranking RPM limit ("crankingRPM_F")
                                      ;*****************************************************************************************
005EA1 7F5EA1 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005EA6 7F5EA6 CD 08 00                    ldy   #veBins_E   ; Load index register Y with address of first configurable
                                                            ; constant on buffer RAM page 1 (veBins_E)
005EA9 7F5EA9 EE EA 03 E2                 ldx   $03E2,Y     ; Load Accu X with value in buffer RAM page 1 (offset 994)($03E2)
                                                            ; ("crankingRPM_F", cranking RPM limit)
                                      ;*****************************************************************************************
                                      ; - Convert to crank angle sensor period ticks in 5.12uS resolution
                                      ;*****************************************************************************************
005EAD 7F5EAD CC C3 46                    ldd  #$C346           ; Load accu D with Lo word of  10 cyl RPMk (5.12uS clock tick)
005EB0 7F5EB0 CD 00 23                    ldy  #$0023           ; Load accu Y with Hi word of 10 cyl RPMk (5.12uS clock tick)
005EB3 7F5EB3 11                          ediv                  ; Extended divide (Y:D)/(X)=>Y;Rem=>D "RPMk" / "crankingRPM_F"
                                                                ; = crank angle sensor period in 5.12uS resolution
                                      ;*****************************************************************************************
                                      ; - Compare the limit period with the current period
                                      ;*****************************************************************************************
005EB4 7F5EB4 BD 10 A2                    cpy   CASprd512       ; Compare cranking RPM limit period to "CASprd512"
005EB7 7F5EB7 25 28                           blo   StillCranking   ; Period is greater than that for "crankingRPM_F" so engine is
                                                                    ; still cranking. Branch to StillCranking:
005EB9 7F5EB9 20 00                       bra   SwitchToRun     ; Branch to SwitchToRun:
005EBB 7F5EBB                         SwitchToRun:
005EBB 7F5EBB 18 0B 7F 00 6E              movb #$7F,ECT_PTPSR   ; Load ECT_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
005EC0 7F5EC0 18 0B 7F 03 FE              movb #$7F,TIM_PTPSR   ; (TIM_PTPSR equ $03FE) Load TIM_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
005EC5 7F5EC5 1C 10 EB 04                 bset  engine,run      ; Set "run" bit of "engine" variable
005EC9 7F5EC9 1D 10 EB 02                 bclr  engine,crank    ; Clear "crank" bit of "engine" variable
005ECD 7F5ECD 1D 10 EC 01                 bclr engine2,base512  ; Clear the "base512" bit of "engine" bit field
005ED1 7F5ED1 1C 10 EC 02                 bset engine2,base256  ; Set the "base256" bit of "engine" bit field
005ED5 7F5ED5 18 79 10 A2                 clrw  CASprd512       ; Clear "CASprd512"
