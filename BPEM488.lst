                                      ;KpHcalc:    equ $02   ; %00000010 (Bit 1) (Do VSS calculations flag)
                                      ;Ch7_2nd:    equ $04   ; %00000100 (Bit 2) (Ch7 2nd edge flag)
                                      ;Ch6alt:     equ $08   ; %00001000 (Bit 3) (Ch6 alt flag)
                                      ;Ch7_3d:     equ $10   ; %00010000 (Bit 4) (Ch7 3d edge flag)
