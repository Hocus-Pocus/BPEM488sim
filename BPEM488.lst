00618A 7F618A FC 10 27                    ldd  CrvRowOfst   ; Offset from the curve page to the curve row -> D         (CRV_SETUP)
00618D 7F618D 19 EE                       leay D,Y          ; Curve row pointer -> Y                                   (CRV_SETUP)
00618F 7F618F 18 05 EE 10 31              movw D,Y,CrvRowLo ; Copy to curve row low boundry value for interpolation    (CRV_SETUP)
006194 7F6194 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation   (CRV_SETUP)
                                                            ; (start with low and high row bin values equal            (CRV_SETUP)
006199 7F6199 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y (CRV_SETUP)
00619C 7F619C FC 10 29                    ldd  CrvColOfst   ; Offset from the curve page to the curve column -> D      (CRV_SETUP)
00619F 7F619F 19 EE                       leay D,Y          ; Curve column pointer -> Y                                (CRV_SETUP)
0061A1 7F61A1 18 05 EE 10 35              movw D,Y,CrvColLo ; Copy to curve row column boundry value for interpolation (CRV_SETUP)
0061A6 7F61A6 18 05 EE 10 33              movw D,Y,CrvColHi ; Copy to curve column high boundry value for interpolation (CRV_SETUP)
                                                            ; (start with low and high column bin values equal         (CRV_SETUP)
0061AB 7F61AB 79 10 2E                    clr   IndexNum    ; Position in the row or column of the curve comparison value (CRV_SETUP)
                                      ;*****************************************************************************************
                                      ; - CrvRowLo is negative. Now check CrvCmpVal for negative number.
                                      ;*****************************************************************************************
0061AE 7F61AE FE 10 2B                    ldx  CrvCmpVal    ; Curve comparison value -> X
0061B1 7F61B1 18 84 80 00                 andx #$8000       ; Logical AND X with %1000 0000 0000 0000 (CCR N bit set of MSB of
                                                            ; result is set)
0061B5 7F61B5 2B 03                       bmi  CmpValNeg    ; If N bit of CCR is set, branch to CmpValNeg:
                                                            ;(CrvCmpVal is negative)
0061B7 7F61B7 06 62 5B                    job  CmpValPos    ; Jump or branch to CmpValPos: (CrvCmpVal is positive)
0061BA 7F61BA                         CmpValNeg:
                                      ;*****************************************************************************************
                                      ; - Both CrvRowLo and CrvCmpVal are negative. Now see if CrvCmpVal is the same or less than
                                      ;   than CrvRowLo. If it is, rail low at the value of the first column bin. If it is not,
                                      ;   it must be greater than CrvRowLo, so loop back to do the next iteration.
                                      ;*****************************************************************************************
0061BA 7F61BA FE 10 2B                    ldx  CrvCmpVal    ; Curve compare value -> X
0061BD 7F61BD BE 10 31                    cpx  CrvRowLo     ; Compare curve compare value with curve low boundary
0061C0 7F61C0 23 02                       bls  RailLowNeg   ; If CrvCmpVal is the same or less than CrvRowLo branch to RailLowNeg:
