005C6F 7F5C6F 1C 10 F3 01                 bset  StateStatus,Synch        ; Set "Synch " bit of "StateStatus" variable (bit0)
005C73 7F5C73 1C 10 F3 04                 bset  StateStatus,StateNew     ; Set "StateNew" bit of "StateStatus" variable (bit2)
005C77 7F5C77 1D 10 F3 02                 bclr  StateStatus,SynchLost    ; Clear "SynchLost" bit of "StateStatus" variable (bit1)
005C7B 7F5C7B 20 0C                       bra   STATE_STATUS_done        ; Branch to STATE_STATUS_done:
005C7D 7F5C7D                         NoLock:
