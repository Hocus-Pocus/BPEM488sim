                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
0062DA 7F62DA                         ReEntCrvPos:
0062DA 7F62DA 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
0062DD 7F62DD 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
0062E3 7F62E3 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
0062E7 7F62E7 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
0062EA 7F62EA FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
0062ED 7F62ED 19 EE                       leay D,Y          ; Curve row pointer -> Y
0062EF 7F62EF 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
