00552B 7F552B CE 00 64                        ldx  #$0064       ; Decimal 100 -> Accu X                                (TOE_OFC_CALCS)
00552E 7F552E 18 10                       idiv              ;(D)/(X)->X:rem->D (reqFuel" x "TOEpct")/10                (TOE_OFC_CALCS)
005530 7F5530 7E 10 C4                    stx  TOEpw        ; Result -> "TOEpw" TOE adder (mS x 10)                    (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
                                      ; - Check to see if Throttle Opening Enrichment is done.                         (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
005533 7F5533                          TOE_CHK_TIME:                                                                   (TOE_OFC_CALCS)
005533 7F5533 1E 10 EB 40 07              brset  engine,OFCon,RESET_TOE ; If Overrun Fuel Cut bit of "Engine" bit field is set, (TOE_OFC_CALCS)
                                                                        ; branch to RESET_TOE:                         (TOE_OFC_CALCS)
005538 7F5538 B6 10 D7                     ldaa  TOEdurCnt    ; "TOEdurCnt" -> Accu A                                  (TOE_OFC_CALCS)
