                                                                    ; CASDone:
005D0B 7F5D0B FC 10 A6                    ldd   CASprd512       ; "CASprd512"-> Accu D (CAS period in 5.12uS resolution)
005D0E 7F5D0E 8C 1E 84                        cpd   #$1E84          ; Compare with decimal 7812 ("CASprdtk" for 300 RPM @5.12uS
                                                                    ; time base)
005D11 7F5D11 22 28                           bhi   StillCranking   ; Period is greater than that for 300 RPM so engine is still
                                                                    ; cranking. Branch to StillCranking:
005D13 7F5D13 20 00                       bra   SwitchToRun     ; Branch to SwitchToRun:
005D15 7F5D15                         SwitchToRun:
005D15 7F5D15 18 0B 7F 00 6E              movb #$7F,ECT_PTPSR   ; Load ECT_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
005D1A 7F5D1A 18 0B 7F 03 FE              movb #$7F,TIM_PTPSR   ; (TIM_PTPSR equ $03FE) Load TIM_PTPSR with %01111111
                                                                ; (prescale 128, 2.56us resolution,
                                                                ; max period 167.7696ms)
005D1F 7F5D1F 1C 10 EF 04                 bset  engine,run      ; Set "run" bit of "engine" variable
005D23 7F5D23 1D 10 EF 02                 bclr  engine,crank    ; Clear "crank" bit of "engine" variable
005D27 7F5D27 1D 10 F0 01                 bclr engine2,base512  ; Clear the "base512" bit of "engine" bit field
005D2B 7F5D2B 1C 10 F0 02                 bset engine2,base256  ; Set the "base256" bit of "engine" bit field
005D2F 7F5D2F 18 79 10 A6                 clrw  CASprd512       ; Clear "CASprd512"
