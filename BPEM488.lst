                                      ;*****************************************************************************************
                                      ; - The camshaft position sensor and the crankshaft position sensor are both hall
                                      ;   effect gear tooth sensors. They read notched wheels on their repsective shafts.
                                      ;   When the sensor senses a notch its output pin goes to ground. The BPEM simulator
                                      ;   input from the sensor is the LED circuit of an opto isolator. When the LED in the
                                      ;   opto is powered it biases the output transistor on so the timer channel pin sees a
                                      ;   rising edge, which triggers an interrupt event. The state machine uses these events
                                      ;   to de-code the signals to determine crankshaft position and camshaft phase. Any
                                      ;   event that does not fall into the mechanical order of events triggers an error.
                                      ;   An error will disable ignition and fuel injection until a positive lock on crankshaft
                                      ;   position and camshaft phase is re-established.
                                      ;*****************************************************************************************
005D60 7F5D60                         ECT_TC5_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT_TC5_ISR Interrupt Service Routine (Camshaft sensor notch)
                                      ;   Event = 0
                                      ;*****************************************************************************************
005D60 7F5D60 DE 5A                       ldx    ECT_TC5H         ; Read ECT_TC5H to clear the flag
005D62 7F5D62 CE 6D 81                    ldx    #StateLookup     ; Load index register X with the address of "TableLookup"
005D65 7F5D65 F6 10 14                    ldab   State            ; Load Accu B with the contents of "State"
005D68 7F5D68 58                          aslb                    ; Shift Accu B 1 place to the left
005D69 7F5D69 CA 00                       orab   #$00             ; Bit wise inclusive OR Accu B with 0
005D6B 7F5D6B A6 E5                       ldaa   B,X              ; Load Accu A with the contents of "TableLookup", offset in
                                                                  ; Accu B (9 bit constant offset indexed addressing)
005D6D 7F5D6D 7A 10 14                    staa   State            ; Copy to "State"
005D70 7F5D70 0B                          rti                     ; Return from interrupt
005D71 7F5D71                         ECT_TC7_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT_TC7_ISR Interrupt Service Routine (Crankshaft sensor notch)
                                      ;   Event = 1
                                      ;*****************************************************************************************
005D71 7F5D71 DE 5E                       ldx    ECT_TC7H              ; Read ECT_TC7H to clear the flag
005D73 7F5D73 CE 6D 81                    ldx    #StateLookup     ; Load index register X with the address of "TableLookup"
005D76 7F5D76 F6 10 14                    ldab   State            ; Load Accu B with the contents of "State"
005D79 7F5D79 58                          aslb                    ; Shift Accu B 1 place to the left
005D7A 7F5D7A CA 01                       orab   #$01             ; Bit wise inclusive OR Accu B with 1
005D7C 7F5D7C A6 E5                       ldaa   B,X              ; Load Accu A with the contents of "TableLookup", offset in
                                                                  ; Accu B (9 bit constant offset indexed addressing)
005D7E 7F5D7E 7A 10 14                    staa   State            ; Copy to "State"
005D81 7F5D81 81 46                       cmpa    #$46            ; Compare with decimal 70 (Error)
005D83 7F5D83 27 06                       beq     State_Error     ; If "State" = $46, branch to State_Error:
005D85 7F5D85 81 67                       cmpa    #$67            ; Compare with decimal 103
005D87 7F5D87 2F 21                       ble     NoLock          ; If "State" =< $67, branch to NoLock:
005D89 7F5D89 2E 11                       bgt     SynchLock       ; If "State" is > $67, branch to Synchlock:
005D8B 7F5D8B                         State_Error:
                                      ;*****************************************************************************************
                                      ; - If we get here we have experienced an unexpected cam or crank input and have lost lock.
                                      ;   No more spark or injection events until lock has been re-established.
                                      ;*****************************************************************************************
005D8B 7F5D8B 79 10 14                    clr   State                     ; Clear "State"
005D8E 7F5D8E 1C 10 EF 02                 bset  StateStatus,SynchLost     ; Set "SynchLost" bit of "StateStatus" variable (bit1)
005D92 7F5D92 1D 10 EF 01                 bclr  StateStatus,Synch         ; Clear "Synch " bit of "StateStatus" variable (bit0)
005D96 7F5D96 1D 10 EF 04                 bclr  StateStatus,StateNew      ; Clear "StateNew " bit of "StateStatus" variable (bit2)
005D9A 7F5D9A 20 1A                       bra   STATE_STATUS_done         ; Branch to STATE_STATUS_done:
005D9C 7F5D9C                         SynchLock:
                                      ;*****************************************************************************************
                                      ; - If we get here we have either just reached one of the four possible lock points, or
                                      ;    we are already in the synch loop.
                                      ;*****************************************************************************************
005D9C 7F5D9C 1C 10 EF 01                 bset  StateStatus,Synch        ; Set "Synch " bit of "StateStatus" variable (bit0)
005DA0 7F5DA0 1C 10 EF 04                 bset  StateStatus,StateNew     ; Set "StateNew" bit of "StateStatus" variable (bit2)
005DA4 7F5DA4 1D 10 EF 02                 bclr  StateStatus,SynchLost    ; Clear "SynchLost" bit of "StateStatus" variable (bit1)
005DA8 7F5DA8 20 0C                       bra   STATE_STATUS_done        ; Branch to STATE_STATUS_done:
005DAA 7F5DAA                         NoLock:
                                      ;*****************************************************************************************
                                      ; - If we get here we have the state machine is still looking for a synch lock.
                                      ;*****************************************************************************************
005DAA 7F5DAA 1D 10 EF 01                 bclr  StateStatus,Synch        ; Clear "Synch" bit of "StateStatus" variable (bit0)
005DAE 7F5DAE 1D 10 EF 02                 bclr  StateStatus,SynchLost    ; Clear "SynchLost" bit of "StateStatus" variable (bit1)
005DB2 7F5DB2 1D 10 EF 04                 bclr  StateStatus,StateNew     ; Clear "StateNew" bit of "StateStatus" variable (bit 2)
005DB6 7F5DB6                         STATE_STATUS_done:
                                      ;*****************************************************************************************
                                      ; - Get three consecutive rising edge signals for engine RPM and
                                      ;   calculate the period. This period is for one fifth of a revolution (72 degrees).
                                      ;   RPM, Ignition and  Fuel calculations are done in the main loop.
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Reload stall counter with compare value. Stall check is done in the main loop every
                                      ;   mSec. "Stallcnt" is decremented every mSec and reloaded at every crank signal.
                                      ;*****************************************************************************************
005DB6 7F5DB6 18 0B FF 00 17                  movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE
005DBB 7F5DBB CD 08 00                    ldy   #veBins_E       ; Load index register Y with address of first configurable
                                                              ; constant on buffer RAM page 1 (vebins)
005DBE 7F5DBE EC EA 03 E6                 ldd   $03E6,Y       ; Load Accu A with value in buffer RAM page 1 offset 998
                                                              ; "Stallcnt" (stall counter)(offset = 998)
005DC2 7F5DC2 7C 10 22                    std  Stallcnt       ; Copy to "Stallcnt" (no crank or stall condition counter)
                                                              ; (1mS increments)
005DC5 7F5DC5 1E 10 24 04 12              brset ICflgs,Ch7_2nd,CAS_2nd ; If "Ch7_2nd" bit of "ICflgs" is set, branch to "CAS_2nd:"
005DCA 7F5DCA 1E 10 24 10 24              brset ICflgs,Ch7_3d,CAS_3d   ; If "Ch7_3d" bit of "ICflgs" is set, branch to "CAS_3d:"
005DCF 7F5DCF DC 5E                       ldd   ECT_TC7H               ; Load accu D with value in "ECT_TC7H"
005DD1 7F5DD1 7C 10 15                    std   CAS1sttk               ; Copy to "CAS1sttk"
005DD4 7F5DD4 1C 10 24 04                 bset  ICflgs,Ch7_2nd         ; Set "Ch7_2nd" bit of "ICflgs"
005DD8 7F5DD8 18 20 00 B0                 bra   CASDone                ; Branch to CASDone:
005DDC 7F5DDC                         CAS_2nd:
005DDC 7F5DDC DC 5E                       ldd   ECT_TC7H        ; Load accu D with value in "ECT_TC7H"
005DDE 7F5DDE 7C 10 17                    std   CAS2ndtk        ; Copy to "CAS2ndtk"
005DE1 7F5DE1 B3 10 15                    subd  CAS1sttk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS1sttk" from value in "ECT_TC7H"
005DE4 7F5DE4 7C 10 19                    std   CASprd1tk       ; Copy result to "CASprd1tk"
005DE7 7F5DE7 1D 10 24 04                 bclr  ICflgs,Ch7_2nd  ; Clear "Ch7_2nd" bit of "ICflgs"
005DEB 7F5DEB 1C 10 24 10                 bset  ICflgs,Ch7_3d   ; Set "Ch7_3d" bit of "ICflgs"
005DEF 7F5DEF 18 20 00 99                 bra   CASDone         ; Branch to CASDone:
005DF3 7F5DF3                         CAS_3d:
005DF3 7F5DF3 DC 5E                       ldd   ECT_TC7H        ; Load accu D with value in "ECT_TC7H"
005DF5 7F5DF5 B3 10 17                    subd  CAS2ndtk        ; Subtract (A:B)-(M:M+1)=>A:B "CAS2ndtk" from value in "ECT_TC7H"
005DF8 7F5DF8 7C 10 1B                    std   CASprd2tk       ; Copy result to "CASprd2tk"
005DFB 7F5DFB F3 10 19                    addd  CASprd1tk       ; (A:B)+(M:M+1)_->A:B "CASprd2tk" + "CASprd1tk" = "CASprdtk"
005DFE 7F5DFE 1D 10 24 10                 bclr  ICflgs,Ch7_3d   ; Clear "Ch7_3d" bit of "ICflgs"
                                      ;*****************************************************************************************
                                      ; - All calculations that use the Crank Angle Sensor period need to know what the
                                      ;   resolution is. The timers are initalized with a 5.12uS resoluion but switched to
                                      ;   2.56uS resolution when the engine tranistions from crank mode to run mode.
                                      ;*****************************************************************************************
005E02 7F5E02 1E 10 EB 04 17              brset engine,run,CAS256 ; If "run" bit of "engine" bit field is set branch to
                                                                    ; CAS256:
005E07 7F5E07 7C 10 A2                    std   CASprd512       ; Copy result to "CASprd512" (CAS period in 5.12uS resolution)
                                      ;******************************************************************************************
                                      ; - Convert Crank Angle Sensor period (5.12uS res)to degrees x 10 of rotation (for 1 tenth
                                      ;   of a degree resolution calculations).("Degx10tk512")
                                      ;******************************************************************************************
005E0A 7F5E0A CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005E0D 7F5E0D 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd512/72)
005E0F 7F5E0F B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005E11 7F5E11 CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005E14 7F5E14 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk512"
