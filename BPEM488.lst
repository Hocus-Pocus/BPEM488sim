005C14 7F5C14 1C 10 24 08                 bset ICflgs,Ch6alt       ; Set "Ch6alt" bit of "ICflgs"
005C18 7F5C18 20 10                       bra  ECT6_ISR_Done       ; Branch to "ECT6_ISR_Done:"
005C1A 7F5C1A                         VSS2:
005C1A 7F5C1A DC 5C                       ldd  ECT_TC6H       ; Load accu D with value in "ECT_TC6H"
005C1C 7F5C1C B3 10 12                    subd VSS1st         ; Subtract (A:B)-(M:M+1)=>A:B "VSS1st" from value in "ECT_TC6H"
