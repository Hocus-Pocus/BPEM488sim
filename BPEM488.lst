005491 7F5491 CD 08 00                    ldy   #veBins_E       ; Load index register Y with address of first configurable (TOE_OFC_CALCS)
                                                              ; constant on buffer RAM page 1 (veBins)                 (TOE_OFC_CALCS)
005494 7F5494 A6 EA 03 D6                 ldaa   $03D6,Y      ; Load Accu A with value in buffer RAM page 1 offset 982 (TOE_OFC_CALCS)
                                                              ; (OFCdel_F) (Overrun Fuel Cut delay time)               (TOE_OFC_CALCS)
005498 7F5498 7A 10 47                    staa  OFCdel        ; Copy to "OFCdel" (Overrun Fuel Cut delay duration)(decremented (TOE_OFC_CALCS)
                                                                  ; every 100mS in rti_BPEM488.s)                      (TOE_OFC_CALCS)
00549B 7F549B 1C 10 EF 01                     bset  engine,OFCdelon ; Set "OFCdelon" bit of "engine" bit field         (TOE_OFC_CALCS)
00549F 7F549F 20 19                       bra   OFC_LOOP      ; Branch to OFC_LOOP: (fall through)                     (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
                                      ; - We have permissives for OFC. We are waiting for the OFC timer to time out. Check to (TOE_OFC_CALCS)
                                      ;   see if "OFCdel" (Overrun Fuel Cut delay duration) has been decremented to zero. (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
0054A1 7F54A1                         OFC_DELAY:                                                                       (TOE_OFC_CALCS)
0054A1 7F54A1 B6 10 47                    ldaa OFCdel     ; "OFCdel" -> Accu A                                         (TOE_OFC_CALCS)
0054A4 7F54A4 27 02                           beq  SET_OFC    ; If "OFCdel" = zero branch to SET_OFC:                  (TOE_OFC_CALCS)
0054A6 7F54A6 20 12                       bra  OFC_LOOP   ; (Branch to OFC_LOOP: (Timer not timed out so fall through) (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
