005882 7F5882                                                 ORG     RTI_CODE_START, RTI_CODE_START_LIN
                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
005882 7F5882                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
005882 7F5882                         Inc125uS:
005882 7F5882 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
005885 7F5885 B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
005888 7F5888 81 08                       cmpa #$08          ; Compare it with decimal 8
00588A 7F588A 26 50                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
00588C 7F588C                         DomS:
00588C 7F588C 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
005890 7F5890 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
005893 7F5893 72 10 03                    inc  mS            ; Increment Millisecond counter
005896 7F5896 B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
005899 7F5899 81 64                       cmpa #$64          ; Compare it with decimal 100
00589B 7F589B 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
00589D 7F589D 81 FA                       cmpa #$FA          ; Compare it with decimal 250
00589F 7F589F 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
0058A1 7F58A1 26 39                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
0058A3 7F58A3                         Do100mS:
0058A3 7F58A3 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
0058A7 7F58A7 20 33                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0058A9 7F58A9                         Do250mS:
0058A9 7F58A9 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
                                      ;   if it's time to do the "500mS" section.
                                      ;*****************************************************************************************
0058AD 7F58AD 79 10 03                    clr  mS            ; Clear Millisecond counter
0058B0 7F58B0 72 10 04                    inc  mSx250        ; Increment 250 Millisecond counter
0058B3 7F58B3 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
0058B6 7F58B6 81 02                       cmpa #$02          ; Compare with decimal 2
0058B8 7F58B8 27 06                       beq  Do500mS       ; If the Z bit of CCR is set, branch to Do500mS:
0058BA 7F58BA 81 04                       cmpa #$04          ; Compare with decimal 4
0058BC 7F58BC 27 02                       beq  Do500mS       ; If the Z bit of CCR is set,branch to Do500mS:
0058BE 7F58BE 20 1C                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 500 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0058C0 7F58C0                         Do500mS:
0058C0 7F58C0 1C 10 05 08                 bset clock,ms500   ; Set "ms500" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Check to see if it's time to do the "Seconds" section
                                      ;*****************************************************************************************
0058C4 7F58C4 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
0058C7 7F58C7 81 04                       cmpa #$04          ; Compare with decimal 4
0058C9 7F58C9 27 02                       beq  DoSec         ; If the Z bit of CCR is set, branch to DoSec:
0058CB 7F58CB 20 0F                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ---------------------------------- Seconds section -------------------------------------
                                      ;*****************************************************************************************
0058CD 7F58CD                         DoSec:
0058CD 7F58CD 1C 10 05 10                 bset clock,ms1000     ; Set "ms1000" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 250 millisecond counter. Increment "secL". Increment "secH" on roll over
                                      ;*****************************************************************************************
0058D1 7F58D1                         IncSec:
                                      ;*****************************************************************************************
                                      ; - Flash LED2 on board 1 to 28 every second just to show that the timer is working
                                      ;*****************************************************************************************
                                      ;    ldaa  PORTB        ; Load ACC A with value in Port B
                                      ;    eora  #$08         ; Exlusive or with $00001000
                                      ;    staa   PORTB       ; Copy to Port B (toggle Bit3, LED20, board 1 to 28)
                                      ;*****************************************************************************************
0058D1 7F58D1 79 10 04                    clr  mSx250        ; Clear 250 mSec counter
0058D4 7F58D4 72 10 69                    inc  secL          ; Increment "Seconds" Lo byte
0058D7 7F58D7 26 03                       bne  RTI_ISR_DONE  ; If the Z bit of CCR is clear, branch to RTI_ISR_DONE:
0058D9 7F58D9 72 10 68                    inc  secH          ; Increment "Seconds" Hi byte
0058DC 7F58DC                         RTI_ISR_DONE:
0058DC 7F58DC 4C 37 80                    bset CRGFLG,RTIF   ; Set "RTIF" bit of "CRGFLG" to clear flag
0058DF 7F58DF 0B                          rti                ; Return from interrupt
                                      ;*****************************************************************************************
0058E0 7F58E0 -> $58E0                RTI_CODE_END            EQU     *     ; * Represents the current value of the paged
                                                                    ; program counter
0058E0 7F58E0 -> $7F58E0              RTI_CODE_END_LIN        EQU     @     ; @ Represents the current value of the linear
                                                                    ; program counter
