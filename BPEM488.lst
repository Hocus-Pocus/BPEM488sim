                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
0063AF 7F63AF                         ReEntCrvNeg1:
0063AF 7F63AF 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
0063B2 7F63B2 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
0063B8 7F63B8 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
0063BC 7F63BC FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
0063BF 7F63BF FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
0063C2 7F63C2 19 EE                       leay D,Y          ; Curve row pointer -> Y
0063C4 7F63C4 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
