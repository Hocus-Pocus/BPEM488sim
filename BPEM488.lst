0058B6 7F58B6                                                 ORG     RTI_CODE_START, RTI_CODE_START_LIN
                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
0058B6 7F58B6                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
0058B6 7F58B6                         Inc125uS:
0058B6 7F58B6 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
0058B9 7F58B9 B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
0058BC 7F58BC 81 08                       cmpa #$08          ; Compare it with decimal 8
0058BE 7F58BE 26 50                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0058C0 7F58C0                         DomS:
0058C0 7F58C0 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
0058C4 7F58C4 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
0058C7 7F58C7 72 10 03                    inc  mS            ; Increment Millisecond counter
0058CA 7F58CA B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
0058CD 7F58CD 81 64                       cmpa #$64          ; Compare it with decimal 100
0058CF 7F58CF 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
0058D1 7F58D1 81 FA                       cmpa #$FA          ; Compare it with decimal 250
0058D3 7F58D3 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
0058D5 7F58D5 26 39                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
0058D7 7F58D7                         Do100mS:
0058D7 7F58D7 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
0058DB 7F58DB 20 33                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0058DD 7F58DD                         Do250mS:
0058DD 7F58DD 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
