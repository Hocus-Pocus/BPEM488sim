                                      ;******************************************************************************************
005CF4 7F5CF4 CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005CF7 7F5CF7 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd256/72)
005CF9 7F5CF9 B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005CFB 7F5CFB CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005CFE 7F5CFE 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk256"
005CFF 7F5CFF 7C 10 1F                        std   Degx10tk256    ; Copy result to "Degx10tk256"
