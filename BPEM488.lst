0063F4 7F63F4 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y (COL_BOUNDARYS)
0063F7 7F63F7 FC 10 29                    ldd  CrvColOfst   ; Offset from the curve page to the curve column -> D      (COL_BOUNDARYS)
0063FA 7F63FA 19 EE                       leay D,Y          ; Curve column pointer -> Y                                (COL_BOUNDARYS)
0063FC 7F63FC 18 05 EE 10 35              movw D,Y,CrvColLo ; Copy to curve column low boundry value for interpolation (COL_BOUNDARYS)
