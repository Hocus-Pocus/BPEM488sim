0058A4 7F58A4                                                 ORG     RTI_CODE_START, RTI_CODE_START_LIN
                                      ;*****************************************************************************************
                                      ; - RTI_ISR Interrupt Service Routine (125 uS clock tick)
                                      ; - Generate time rates:
                                      ;   125 Microseconds
                                      ;   1 Millisecond
                                      ;   100 Milliseconds
                                      ;   250 Millisecnds
                                      ;   500 Milliseconds
                                      ;   Seconds
                                      ;*****************************************************************************************
0058A4 7F58A4                         RTI_ISR:
                                      ;*****************************************************************************************
                                      ; ------------------------------ 125 Microsecond section ---------------------------------
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Increment 125 microsecond counter and check to see if it's time to do the
                                      ; Millisecond section
                                      ;*****************************************************************************************
0058A4 7F58A4                         Inc125uS:
0058A4 7F58A4 72 10 02                    inc  uSx125        ; Increment 125 Microsecond counter
0058A7 7F58A7 B6 10 02                    ldaa uSx125        ; Load accu A with value in 125 uS counter
0058AA 7F58AA 81 08                       cmpa #$08          ; Compare it with decimal 8
0058AC 7F58AC 26 54                       bne  RTI_ISR_DONE  ; If not equal, branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; --------------------------------- Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0058AE 7F58AE                         DomS:
0058AE 7F58AE 1C 10 05 01                 bset clock,ms1     ; Set "ms1" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 125 microsecond counter. Increment millisecond counter and check to see
                                      ;   if it's time to do the 100 Millisecond or 250 Millisecond section.
                                      ;*****************************************************************************************
0058B2 7F58B2 79 10 02                    clr  uSx125        ; Clear 125 Microsecond counter
0058B5 7F58B5 72 10 03                    inc  mS            ; Increment Millisecond counter
0058B8 7F58B8 B6 10 03                    ldaa mS            ; Load accu A with value in mS counter
0058BB 7F58BB 81 64                       cmpa #$64          ; Compare it with decimal 100
0058BD 7F58BD 27 0A                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=100)
0058BF 7F58BF 81 C8                       cmpa #$C8          ; Compare it with decimal 200
0058C1 7F58C1 27 06                       beq  Do100mS       ; IF Z bit of CCR is set, branch to Do100mS: (mS=200)
0058C3 7F58C3 81 FA                       cmpa #$FA          ; Compare it with decimal 250
0058C5 7F58C5 27 08                       beq  Do250mS       ; IF Z bit of CCR is set, branch to Do250mS: (mS=250)
0058C7 7F58C7 26 39                       bne  RTI_ISR_DONE  ; If not equal branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ------------------------------- 100 Millisecond section --------------------------------
                                      ;*****************************************************************************************
0058C9 7F58C9                         Do100mS:
0058C9 7F58C9 1C 10 05 02                bset clock,ms100    ; Set "ms100" bit of "clock" bit field
0058CD 7F58CD 20 33                      bra  RTI_ISR_DONE   ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 250 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0058CF 7F58CF                         Do250mS:
0058CF 7F58CF 1C 10 05 04                 bset clock,ms250   ; Set "ms250" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the millisecond counter. Increment 250 Millisecond counter  and check to see
                                      ;   if it's time to do the "500mS" section.
                                      ;*****************************************************************************************
0058D3 7F58D3 79 10 03                    clr  mS            ; Clear Millisecond counter
0058D6 7F58D6 72 10 04                    inc  mSx250        ; Increment 250 Millisecond counter
0058D9 7F58D9 B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
0058DC 7F58DC 81 02                       cmpa #$02          ; Compare with decimal 2
0058DE 7F58DE 27 06                       beq  Do500mS       ; If the Z bit of CCR is set, branch to Do500mS:
0058E0 7F58E0 81 04                       cmpa #$04          ; Compare with decimal 4
0058E2 7F58E2 27 02                       beq  Do500mS       ; If the Z bit of CCR is set,branch to Do500mS:
0058E4 7F58E4 20 1C                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ----------------------------- 500 Millisecond section ----------------------------------
                                      ;*****************************************************************************************
0058E6 7F58E6                         Do500mS:
0058E6 7F58E6 1C 10 05 08                 bset clock,ms500   ; Set "ms500" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Check to see if it's time to do the "Seconds" section
                                      ;*****************************************************************************************
0058EA 7F58EA B6 10 04                    ldaa mSx250        ; Load accu A with value in 250 mSec counter
0058ED 7F58ED 81 04                       cmpa #$04          ; Compare with decimal 4
0058EF 7F58EF 27 02                       beq  DoSec         ; If the Z bit of CCR is set, branch to DoSec:
0058F1 7F58F1 20 0F                       bra  RTI_ISR_DONE  ; Branch to RTI_ISR_DONE:
                                      ;*****************************************************************************************
                                      ; ---------------------------------- Seconds section -------------------------------------
                                      ;*****************************************************************************************
0058F3 7F58F3                         DoSec:
0058F3 7F58F3 1C 10 05 10                 bset clock,ms1000     ; Set "ms1000" bit of "clock"
                                      ;*****************************************************************************************
                                      ; - Clear the 250 millisecond counter. Increment "secL". Increment "secH" on roll over
                                      ;*****************************************************************************************
0058F7 7F58F7                         IncSec:
0058F7 7F58F7 79 10 04                    clr  mSx250        ; Clear 250 mSec counter
0058FA 7F58FA 72 10 65                    inc  secL          ; Increment "Seconds" Lo byte
0058FD 7F58FD 26 03                       bne  RTI_ISR_DONE  ; If the Z bit of CCR is clear, branch to RTI_ISR_DONE:
0058FF 7F58FF 72 10 64                    inc  secH          ; Increment "Seconds" Hi byte
005902 7F5902                         RTI_ISR_DONE:
005902 7F5902 4C 37 80                    bset CRGFLG,RTIF   ; Set "RTIF" bit of "CRGFLG" to clear flag
005905 7F5905 0B                          rti                ; Return from interrupt
                                      ;*****************************************************************************************
005906 7F5906 -> $5906                RTI_CODE_END            EQU     *     ; * Represents the current value of the paged
                                                                    ; program counter
005906 7F5906 -> $7F5906              RTI_CODE_END_LIN        EQU     @     ; @ Represents the current value of the linear
                                                                    ; program counter
