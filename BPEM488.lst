0064D2 7F64D2 34                          pshx              ; Save to stack                                            (CRV_INTERP)
0064D3 7F64D3 FE 10 33                    ldx  CrvColHi     ; Curve column high boundry value -> X                     (CRV_INTERP)
0064D6 7F64D6 34                          pshx              ; Save to stack                                            (CRV_INTERP)
0064D7 7F64D7 FE 10 35                    ldx  CrvColLo     ; Curve column low boundry value -> X                      (CRV_INTERP)
0064DA 7F64DA 34                          pshx              ; Save to stack                                            (CRV_INTERP)
