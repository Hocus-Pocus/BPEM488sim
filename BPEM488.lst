                                                                ; (prescale 256, 5.12us resolution,
                                                                ; max period 335.5ms)
005EE6 7F5EE6 18 0B FF 03 FE              movb #$FF,TIM_PTPSR   ; Load TIM_PTPSR with %11111111
                                                                ; (prescale 256, 5.12us resolution,
                                                                ; max period 335.5ms)
