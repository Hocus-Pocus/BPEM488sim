005CFC 7F5CFC                         ECT_TC0_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT ch0 Interrupt Service Routine (for (D7)(1to28)(Ign1)(1&6) control)
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Set the output compare value for desired on time and disable the interrupt
                                      ;*****************************************************************************************
005CFC 7F5CFC 4C 49 02                    bset ECT_TCTL2,Bit1    ; Clear Ch0 output line to zero on compare
005CFF 7F5CFF 4D 49 01                    bclr ECT_TCTL2,Bit0    ; Clear Ch0 output line to zero on compare
005D02 7F5D02 DC 44                       ldd  ECT_TCNTH         ; Contents of Timer Count Register-> Accu D
005D04 7F5D04 F3 10 43                    addd IgnOCadd2         ; Add "IgnOCadd2" (dwell time)
005D07 7F5D07 5C 50                       std  ECT_TC0H          ; Copy result to Timer IC/OC register 0 (Start OC operation)
                                                                 ; (coil on for dwell time)(LED on)
005D09 7F5D09 0B                          rti                    ; Return from Interrupt
                                      ;*****************************************************************************************
                                      ; - NOTE! ECT_TC1_ISR is not enabled
                                      ;*****************************************************************************************
005D0A 7F5D0A                         ECT_TC2_ISR:
                                      ;*****************************************************************************************
                                      ; - ECT ch2 Interrupt Service Routine (for (D8)(1to28)(Ign2)(10&5) control)
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Set the output compare value for desired on time and disable the interrupt
                                      ;*****************************************************************************************
005D0A 7F5D0A 4C 49 20                    bset ECT_TCTL2,Bit5    ; Clear Ch2 output line to zero on compare
005D0D 7F5D0D 4D 49 10                    bclr ECT_TCTL2,Bit4    ; Clear Ch2 output line to zero on compare
005D10 7F5D10 DC 44                       ldd  ECT_TCNTH         ; Contents of Timer Count Register-> Accu D
005D12 7F5D12 F3 10 43                    addd IgnOCadd2         ; Add "IgnOCadd2" (dwell time))
005D15 7F5D15 5C 54                       std  ECT_TC2H          ; Copy result to Timer IC/OC register 2(Start OC operation)
                                                                 ; (coil on for dwell time)(LED on)
005D17 7F5D17 0B                          rti                    ; Return from Interrupt
                                      ;*****************************************************************************************
                                      ; - NOTE! ECT_TC3_ISR is not enabled
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - NOTE! ECT_TC4_ISR is not enabled
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - NOTE! ECT_TC5_ISR (camshaft position sensor) is handled in state_BEEM488.s module)
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - ECT ch6 Interrupt Service Routine (for VSS calculations)
                                      ;*****************************************************************************************
005D18 7F5D18                         ECT_TC6_ISR:
                                      ;*****************************************************************************************
                                      ; - Get two consecutive rising edge signals for vehicle speed and
                                      ;   calculate the period. KPH calculations are done in the main loop
                                      ;*****************************************************************************************
005D18 7F5D18 1E 10 24 08 0B              brset ICflgs,Ch6alt,VSS2 ; If "Ch6alt" bit of "ICflgs" is set, branch to "VSS2:"
005D1D 7F5D1D DC 5C                       ldd  ECT_TC6H            ; Load accu D with value in "ECT_TC6H"
005D1F 7F5D1F 7C 10 12                    std  VSS1st              ; Copy to "VSS1st"
005D22 7F5D22 1C 10 24 08                 bset ICflgs,Ch6alt       ; Set "Ch6alt" bit of "ICflgs"
005D26 7F5D26 20 10                       bra  ECT6_ISR_Done       ; Branch to "ECT6_ISR_Done:"
005D28 7F5D28                         VSS2:
005D28 7F5D28 DC 5C                       ldd  ECT_TC6H       ; Load accu D with value in "ECT_TC6H"
005D2A 7F5D2A B3 10 12                    subd VSS1st         ; Subtract (A:B)-(M:M+1)=>A:B "VSS1st" from value in "ECT_TC6H"
005D2D 7F5D2D 7C 10 A6                    std  VSSprd         ; Copy result to "VSSprd"
005D30 7F5D30 1D 10 24 08                 bclr ICflgs,Ch6alt  ; Clear "Ch6alt" bit of "ICflgs"
005D34 7F5D34 1C 10 24 02                 bset ICflgs,KPHcalc ; Set "KPHcalc" bit of "ICflgs"
005D38 7F5D38                         ECT6_ISR_Done:
005D38 7F5D38 0B                          rti                ; Return from Interrupt
