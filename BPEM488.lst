00626B 7F626B 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
006271 7F6271 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
006275 7F6275 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
006278 7F6278 FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
00627B 7F627B 19 EE                       leay D,Y          ; Curve row pointer -> Y
00627D 7F627D 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
                                                            ; (holds the contents of the incremented row bin)
006282 7F6282 FE 10 2F                    ldx  CrvRowHi     ; Curve row high boundry value -> X
006285 7F6285 18 84 80 00                 andx #$8000       ; Logical AND X with %1000 0000 0000 0000 (CCR N bit set of MSB of
                                                            ; result is set)
006289 7F6289 2B DD                       bmi  PosFind      ; If N bit of CCR is set, branch to PosFind: (CrvRowHi is negative
                                                            ; so loop back until the first positive value is found)
                                      ;*****************************************************************************************
                                      ; - CrvCmpVal is positive. We have found the first positive row value so all other row
                                      ;   values will be positive.
                                      ;*****************************************************************************************
                                      ;*****************************************************************************************
                                      ; - Both CrvRowLo and CrvCmpVal are positive. CrvCmpVal is the greater than CrvRowLo.
                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
00628B 7F628B                         ReEntCrvPos1:
00628B 7F628B 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
00628E 7F628E 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
006294 7F6294 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
006298 7F6298 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
00629B 7F629B FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
00629E 7F629E 19 EE                       leay D,Y          ; Curve row pointer -> Y
0062A0 7F62A0 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
                                                            ; (holds the contents of the incremented row bin)
                                      ;*****************************************************************************************
