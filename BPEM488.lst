005503 7F5503 B7 50                       tfr   X,A            ; idiv result -> A                                      (TOE_OFC_CALCS)
005505 7F5505 42                          inca                 ; idiv result + 1 -> A (round up)                       (TOE_OFC_CALCS)
005506 7F5506 20 02                       bra   ADD_COLDADD    ; Branch to ADD_COLDADD:(fall through)                  (TOE_OFC_CALCS)
005508 7F5508                         NO_ROUND_UP:                                                                     (TOE_OFC_CALCS)
005508 7F5508 B7 50                       tfr   X,A            ; idiv result -> A                                      (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
                                      ; - Add the result with "ColdAddpct". Compare the result with the current "TOEpct" and (TOE_OFC_CALCS)
