                                                              ; precision timer)                                       (BASE_INIT/INIT_ECT)
00436C 7F436C 18 0B FF 00 4C              movb  #$FF,ECT_TIE  ; Load Timer Interrupt Enable Register                   (BASE_INIT/INIT_ECT)
