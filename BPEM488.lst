                                      ;***************************************************************************************** (TOE_OFC_CALCS)
0055B2 7F55B2                         OFC_DELAY:                                                                       (TOE_OFC_CALCS)
0055B2 7F55B2 B6 10 D6                    ldaa OFCdelCnt     ; "OFCdelCnt" -> Accu A                                   (TOE_OFC_CALCS)
0055B5 7F55B5 27 02                           beq  SET_OFC       ; If "OFCdelCnt" = zero branch to SET_OFC:            (TOE_OFC_CALCS)
0055B7 7F55B7 20 12                       bra  OFC_LOOP   ; (Branch to OFC_LOOP: (Timer not timed out so fall through) (TOE_OFC_CALCS)
                                      ;***************************************************************************************** (TOE_OFC_CALCS)
