                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
006328 7F6328                         ReEntCrvPos1:
006328 7F6328 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
00632B 7F632B 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
006331 7F6331 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
006335 7F6335 FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
006338 7F6338 FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
00633B 7F633B 19 EE                       leay D,Y          ; Curve row pointer -> Y
00633D 7F633D 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
                                                            ; (holds the contents of the incremented row bin)
                                      ;*****************************************************************************************
