                                      ;*********************************************************************************************** (TOE_OFC_CALCS)
005333 7F5333 FC 10 6A                    ldd  cltADC       ; "cltADC" -> D                                            (TOE_OFC_CALCS)
005336 7F5336 8C 00 93                    cpd  #$0093       ; Compare "cltADC" with decimal 147(ADC @ 179.9F)          (TOE_OFC_CALCS)
005339 7F5339 23 02                       bls  RailColdMul  ; If "cltADC" is lower or the same as 147, branch to RailColdMul: (TOE_OFC_CALCS)
00533B 7F533B 20 07                       bra  DoColdMul    ; Branch to DoColdMul: (skip over)                         (TOE_OFC_CALCS)
00533D 7F533D                         RailColdMul:                                                                     (TOE_OFC_CALCS)
00533D 7F533D 18 0B 64 10 C6             movb #$64,ColdMulpct  ; Decimal 100 -> "ColdMulpct" (100% = no multiplier))   (TOE_OFC_CALCS)
005342 7F5342 20 3C                      bra   ColdMulDone     ; Branch to ColdMulDone: (skip over)                    (TOE_OFC_CALCS)
005344 7F5344                         DoColdMul:                                                                       (TOE_OFC_CALCS)
005344 7F5344 CC 00 93                    ldd  #$0093      ; Load double accumulator with decimal 147 (ADC @ 179.9F)   (TOE_OFC_CALCS)
005347 7F5347 3B                          pshd             ; Push to stack (V1)                                        (TOE_OFC_CALCS)
005348 7F5348 FC 10 6A                    ldd  cltAdc      ; Load double accumulator with "cltAdc"                     (TOE_OFC_CALCS)
00534B 7F534B 3B                          pshd             ; Push to stack (V)                                         (TOE_OFC_CALCS)
00534C 7F534C CC 03 EB                    ldd  #$03EB      ; Load double accumulator with decimal 1003 (ADC @ -39.72F) (TOE_OFC_CALCS)
00534F 7F534F 3B                          pshd             ; Push to stack (V2)                                        (TOE_OFC_CALCS)
005350 7F5350 CC 00 64                    ldd  #$0064      ; Load double accumulator with decimal 100 (multiplier amount at 179.9F) (TOE_OFC_CALCS)
                                                           ;(1.00 multiplier at 180 degrees)                           (TOE_OFC_CALCS)
005353 7F5353 3B                          pshd             ; Push to stack (Z1)                                        (TOE_OFC_CALCS)
005354 7F5354 18 0B FF 00 17              movb  #(BUF_RAM_P1_START>>16),EPAGE  ; Move $FF into EPAGE                   (TOE_OFC_CALCS)
005359 7F5359 CD 08 00                    ldy   #veBins_E    ; Load index register Y with address of first configurable constant (TOE_OFC_CALCS)
