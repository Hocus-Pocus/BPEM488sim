                                      ;******************************************************************************************
005CDD 7F5CDD CE 00 48                    ldx   #$0048         ; Decimal 72 -> X
005CE0 7F5CE0 18 10                       idiv                 ; (D)/(X)->(X)rem(D) (CASprd512/72)
005CE2 7F5CE2 B7 54                       tfr   X,D            ; Copy result in "X" to "D"
005CE4 7F5CE4 CD 00 0A                        ldy   #$000A         ; Decimal 10 -> Accu Y
005CE7 7F5CE7 13                              emul                 ; (D)*(Y)->Y:D result * 10 = "Degx10tk512"
005CE8 7F5CE8 7C 10 1D                        std   Degx10tk512    ; Copy result to "Degx10tk512"
005CEB 7F5CEB 18 79 10 1F                     clrw  Degx10tk256    ; Clear "Degx10tk256"
005CEF 7F5CEF 20 15                           bra   CASprdDone     ; Branch to CASprdDone:
005CF1 7F5CF1                         CAS256:
