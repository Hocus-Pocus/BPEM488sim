                                      ;   Determine the value of CrvRowHi
                                      ;*****************************************************************************************
006100 7F6100                         ReEntCrvPos:
006100 7F6100 72 10 2E                    inc  IndexNum     ; Increment position in the row or column of the curve comparison
                                                            ; value
006103 7F6103 18 04 10 2F 10 31           movw CrvRowHi,CrvRowLo ; Curve row high boundry value -> curve row low boundry value
006109 7F6109 18 72 10 27                 incw CrvRowOfst   ; Increment Offset from the curve page to the curve row
00610D 7F610D FD 10 25                    ldy  CrvPgPtr     ; Pointer to the page where the desired curve resides -> Y
006110 7F6110 FC 10 27                    ldd  CrvRowOfst   ; Incremented offset from the curve page to the curve row -> D
006113 7F6113 19 EE                       leay D,Y          ; Curve row pointer -> Y
006115 7F6115 18 05 EE 10 2F              movw D,Y,CrvRowHi ; Copy to curve row high boundry value for interpolation
