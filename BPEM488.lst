00609C 7F609C 19 EA 02 AC                             LEAY 3DLUT_COL_BIN_OFFSET,Y   ; Column bin pointer -> Y
                                                                            ;($2AC=684)
0060A0 7F60A0 C6 22                           LDAB #(2*(3DLUT_ROW_COUNT-1)) ; Lower column bin offset -> B
                                                                            ;($22=34)
0060A2 7F60A2 18 0F                           TBA                                     ; Lower  offset -> A (start at $22=34)
0060A4 7F60A4 AE EC                           CPX      A,Y            ; Compare column value against current bin
                                                                  ; value
0060A6 7F60A6 2C 10                           BGE  3D_LOOKUP_2A   ; First iteration, if equal to or greater
                                                                  ; than current bin value, rail high, upper
                                                                  ; and lower bin offsets the same
0060A8 7F60A8                         3D_LOOKUP_1:
0060A8 7F60A8 18 0F                           TBA                                     ; Lower  offset -> A
0060AA 7F60AA AE EC                           CPX      A,Y            ; Compare column value against current bin
                                                                  ; value
0060AC 7F60AC 2C 08                                   BGE      3D_LOOKUP_2    ; Branch if column value is greater than
                                                                  ; or equal to current bin value
                                                                  ;(match found)
0060AE 7F60AE 53                                      DECB                ; Decrement bin offset low byte
0060AF 7F60AF 04 31 F6                                DBNE B,3D_LOOKUP_1  ; Decrement bin offset Hi byte and loop
                                                                  ; back if not zero
0060B2 7F60B2 18 0F                                   TBA                                     ; Column value too low, no match found,
                                                                  ; rail low, make lower and upper bin
                                                                  ; offsets the same)
0060B4 7F60B4 20 02                           BRA   3D_LOOKUP_2A
                                      ;*****************************************************************************************
                                      ; - Increment lower offset to make upper offset
                                      ;*****************************************************************************************
0060B6 7F60B6                         3D_LOOKUP_2:
0060B6 7F60B6 42                              INCA                ; Increment lower offset Lo byte
0060B7 7F60B7 42                              INCA                ; Increment lower offset Hi byte to make
