005CA7 7F5CA7 1C 10 24 04                 bset  ICflgs,Ch7_2nd         ; Set "Ch7_2nd" bit of "ICflgs"
005CAB 7F5CAB 18 20 00 B0                 bra   CASDone                ; Branch to CASDone:
