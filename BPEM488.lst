                                      ;******************************************************************************************
                                      ; - Rev counter -
                                      ;   Used to decrement "ASErev" every revolution  (count down counter for ASE taper)
                                      ;******************************************************************************************
005D5F 7F5D5F                         DoRevCntr:
005D5F 7F5D5F B6 10 21                    ldaa  RevCntr        ; Load Accu A with value in "RevCntr"
005D62 7F5D62 81 09                       cmpa  #$09           ; Compare with decimal 9
005D64 7F5D64 27 24                       beq   CAS1           ; If equal branch to CAS1: (First CAS signal)
005D66 7F5D66 81 08                       cmpa  #$08           ; Compare with decimal 8
005D68 7F5D68 27 30                       beq   CAS2           ; If equal branch to CAS2: (Second CAS signal)
005D6A 7F5D6A 81 07                       cmpa  #$07           ; Compare with decimal 7
005D6C 7F5D6C 27 31                       beq   CAS3           ; If equal branch to CAS3: (Third CAS signal)
005D6E 7F5D6E 81 06                       cmpa  #$06           ; Compare with decimal 6
005D70 7F5D70 27 32                       beq   CAS4           ; If equal branch to CAS4: (Forth CAS signal)
005D72 7F5D72 81 05                       cmpa  #$05           ; Compare with decimal 5
005D74 7F5D74 27 33                       beq   CAS5           ; If equal branch to CAS5: (Fifth CAS signal)
005D76 7F5D76 81 04                       cmpa  #$04           ; Compare with decimal 4
005D78 7F5D78 27 34                       beq   CAS6           ; If equal branch to CAS6: (Sixth CAS signal)
005D7A 7F5D7A 81 03                       cmpa  #$03           ; Compare with decimal 3
005D7C 7F5D7C 27 35                       beq   CAS7           ; If equal branch to CAS7: (Seventh CAS signal)
005D7E 7F5D7E 81 02                       cmpa  #$02           ; Compare with decimal 2
005D80 7F5D80 27 36                       beq   CAS8           ; If equal branch to CAS8: (Eighth CAS signal)
005D82 7F5D82 81 01                       cmpa  #$01           ; Compare with decimal 1
005D84 7F5D84 27 37                       beq   CAS9           ; If equal branch to CAS9: (Nineth CAS signal)
005D86 7F5D86 81 00                       cmpa  #$00           ; Compare with zero
005D88 7F5D88 27 38                       beq   CAS10          ; If equal branch to CAS10: (Tenth CAS signal)
005D8A 7F5D8A                         CAS1:
                                      ;*****************************************************************************************
                                      ; - if ASE is in progress, decrement the counter (ASEcnt)
                                      ;*****************************************************************************************
005D8A 7F5D8A 1E 10 EF 08 02              brset engine,ASEon,DecASECnt ; If "ASEon" bit of "engine" bit field is set, branch to
                                                                 ; DecASECnt:
005D8F 7F5D8F 20 04                       bra  NoDecASECnt       ; Branch to NoDecASECnt:
005D91 7F5D91                         DecASECnt:
005D91 7F5D91 18 73 10 B0                 decw   ASEcnt          ; Decrement "ASEcnt"(countdown value for ASE taper)
                                                                 ; (starts with the lookup value of "ASErev")
005D95 7F5D95                         NoDecASECnt:
005D95 7F5D95 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now eight)
005D98 7F5D98 20 31                       bra   RevCntrDone      ; Branch to RevCntrDone:
005D9A 7F5D9A                         CAS2:
005D9A 7F5D9A 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now seven)
005D9D 7F5D9D 20 2C                       bra   RevCntrDone      ; Branch to RevCntrDone:
005D9F 7F5D9F                         CAS3:
005D9F 7F5D9F 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now six)
005DA2 7F5DA2 20 27                       bra   RevCntrDone      ; Branch to RevCntrDone:
005DA4 7F5DA4                         CAS4:
005DA4 7F5DA4 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now five)
005DA7 7F5DA7 20 22                       bra   RevCntrDone      ; Branch to RevCntrDone:
005DA9 7F5DA9                         CAS5:
005DA9 7F5DA9 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now four)
005DAC 7F5DAC 20 1D                       bra   RevCntrDone      ; Branch to RevCntrDone:
005DAE 7F5DAE                         CAS6:
005DAE 7F5DAE 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now three)
005DB1 7F5DB1 20 18                       bra   RevCntrDone      ; Branch to RevCntrDone:
005DB3 7F5DB3                         CAS7:
005DB3 7F5DB3 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now two)
005DB6 7F5DB6 20 13                       bra   RevCntrDone      ; Branch to RevCntrDone:
005DB8 7F5DB8                         CAS8:
005DB8 7F5DB8 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now one)
005DBB 7F5DBB 20 0E                       bra   RevCntrDone      ; Branch to RevCntrDone:
005DBD 7F5DBD                         CAS9:
005DBD 7F5DBD 73 10 21                    dec   RevCntr          ; Decrement "RevCntr"(now zero)
005DC0 7F5DC0 20 09                       bra   RevCntrDone      ; Branch to RevCntrDone:
