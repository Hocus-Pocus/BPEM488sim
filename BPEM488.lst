005454 7F5454 CD 08 00                    ldy   #veBins_E     ; Load index register Y with address of first configurable constant (TOE_OFC_CALCS)
                                                            ; on buffer RAM page 1 (veBins)                            (TOE_OFC_CALCS)
005457 7F5457 EE EA 03 D1                 ldx   $03D1,Y     ; Load X with value in buffer RAM page 1 offset 977 (OFCtps) (TOE_OFC_CALCS)
                                                            ;(Overrun Fuel Cut min TPS%)                               (TOE_OFC_CALCS)
00545B 7F545B BE 10 9E                    cpx  TpsPctx10    ; Compare it with value in "TpsPctx10"                     (TOE_OFC_CALCS)
